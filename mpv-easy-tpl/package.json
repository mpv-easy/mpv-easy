    "@mpv-easy/polyfill": "workspace:*",
    "@mpv-easy/react": "workspace:*",
    "@mpv-easy/tool": "workspace:*",
    "react": "18.3.1"
  },
  "devDependencies": {
    "@babel/cli": "7.25.7",
    "@chokidar-napi/chokidar": "0.1.3",
    "@babel/core": "7.25.7",
    "@babel/preset-env": "7.25.7",
    "@babel/preset-typescript": "7.25.7",
    "esbuild": "0.24.0",
    "tsx": "4.19.1",
    "@types/node": "22.7.4",
    "@types/react": "18.3.11",
    "@types/react-dom": "18.3.0"
