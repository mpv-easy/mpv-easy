  },
  "devDependencies": {
    "@babel/cli": "7.27.2",
    "@babel/core": "7.27.4",
    "@babel/preset-env": "7.27.2",
    "@babel/preset-typescript": "7.27.1",
    "@chokidar-napi/chokidar": "0.1.3",
    "@types/node": "24.0.4",
    "@types/react": "19.1.8",
