{
  "name": "@mpv-easy/easy-react",
  "version": "0.1.13",
  "homepage": "https://github.com/mpv-easy/mpv-easy#readme",
  "files": ["dist", "readme.md", "package.json"],
  "description": "@mpv-easy/easy-react",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "bundle:example": "esbuild ./src/example/*.(tsx|ts) --bundle --outdir=bundle/example --charset=utf8 --define:globalThis.version='\"0.1.13\"' --define:process.env.NODE_ENV='\"production\"'",
    "es5:example": "babel --presets  @babel/preset-env ./bundle/example -d es5/example",
    "bundle": "tsx build.ts",
    "es5": "babel --presets  @babel/preset-env ./bundle -d es5",
    "dev": "chokidar '../mpv-*/src/**/*' -c 'pnpm run build' -d 2000 --initial",
    "dev-copy": "chokidar '../mpv-*/src/**/*' -c 'pnpm run build && pnpm run copy' -d 2000 --initial",
    "dev-ts": "tsc -p ./tsconfig.build.json -w",
    "build": "tsc -p ./tsconfig.build.json && pnpm run bundle && pnpm run es5 && pnpm run meta",
    "build-es6": "esbuild ./src/mpv-easy-es6.tsx --bundle --outdir=bundle --charset=utf8 --define:globalThis.version='\"0.1.13\"' --minify --define:process.env.NODE_ENV='\"production\"'",
    "copy": "cp -r ./es5/mpv-easy.js $MPV_SCRIPT_DIR/mpv-easy.js",
    "meta": "tsx ./meta/build.ts",
    "babel": "minify ./dist/mpv-easy.js --out-file ./dist/input.min.js"
  },
  "keywords": [],
  "author": "ahaoboy <504595380@qq.com> (https://github.com/ahaoboy)",
  "license": "MIT",
  "dependencies": {
    "@mpv-easy/anime": "workspace:*",
    "@mpv-easy/anime4k": "workspace:*",
    "@mpv-easy/assdraw": "workspace:*",
    "@mpv-easy/autoload": "workspace:*",
    "@mpv-easy/clipboard-play": "workspace:*",
    "@mpv-easy/copy-screen": "workspace:*",
    "@mpv-easy/copy-time": "workspace:*",
    "@mpv-easy/crop": "workspace:*",
    "@mpv-easy/cut": "workspace:*",
    "@mpv-easy/flex": "workspace:*",
    "@mpv-easy/i18n": "workspace:*",
    "@mpv-easy/jellyfin": "workspace:*",
    "@mpv-easy/mock": "workspace:*",
    "@mpv-easy/plugin": "workspace:*",
    "@mpv-easy/polyfill": "workspace:*",
    "@mpv-easy/react": "workspace:*",
    "@mpv-easy/thumbfast": "workspace:*",
    "@mpv-easy/tool": "workspace:*",
    "@mpv-easy/translate": "workspace:*",
    "@react-spring/rafz": "9.7.5",
    "@reduxjs/toolkit": "2.5.0",
    "@rematch/core": "2.2.0",
    "@rematch/select": "3.1.2",
    "@tanstack/react-query": "5.62.10",
    "@types/use-sync-external-store": "0.0.6",
    "e-color": "0.1.7",
    "jotai": "2.11.0",
    "lodash-es": "4.17.21",
    "mobx": "6.13.5",
    "mobx-react-lite": "4.1.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-reconciler": "0.31.0",
    "react-redux": "9.2.0",
    "react-use": "17.6.0",
    "redux": "5.0.1",
    "swr": "2.3.0",
    "use-sync-external-store": "1.4.0",
    "zustand": "5.0.2"
  },
  "browserslist": [
    "iOS >= 9",
    "Android >= 4.4",
    "last 2 versions",
    "> 0.2%",
    "not dead"
  ]
}
