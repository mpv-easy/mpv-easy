{
  "name": "@mpv-easy/mpv-easy",
  "version": "0.1.9-alpha.10",
  "files": ["dist", "readme.md", "package.json"],
  "description": "@mpv-easy/easy",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "bundle:example": "esbuild ./src/example/*.(tsx|ts) --bundle --outdir=bundle/example --charset=utf8 --define:globalThis.version='\"0.1.9-alpha.8\"'",
    "es5:example": "babel --presets  @babel/preset-env ./bundle/example -d es5/example --plugins ./src/babel/hack.js",
    "bundle": "esbuild ./src/mpv-easy-*.tsx --bundle --outdir=bundle --charset=utf8 --define:globalThis.version='\"0.1.9\"'",
    "es5": "babel --presets  @babel/preset-env ./bundle -d es5 --plugins ./babel/hack.js",
    "dev": "chokidar '../mpv-*/src/**/*' -c 'pnpm run build && pnpm run copy' -d 2000 --initial",
    "dev-ts": "tsc -p ./tsconfig.build.json -w",
    "build": "tsc -p ./tsconfig.build.json && pnpm run bundle && pnpm run es5 && pnpm run meta",
    "build-es6": "esbuild ./src/mpv-easy-es6.tsx --bundle --outdir=bundle --charset=utf8 --define:globalThis.version='\"0.1.9-alpha.8\"'",
    "copy": "cp -r ./es5/mpv-easy-es5.js $MPV_SCRIPT_DIR/mpv-easy-es5.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "meta": "tsx ./meta/build.ts",
    "babel": "minify ./dist/mpv-easy.js --out-file ./dist/input.min.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@mpv-easy/anime": "workspace:*",
    "@mpv-easy/anime4k": "workspace:*",
    "@mpv-easy/assdraw": "workspace:*",
    "@mpv-easy/autoload": "workspace:*",
    "@mpv-easy/clip-play": "workspace:*",
    "@mpv-easy/copy-screen": "workspace:*",
    "@mpv-easy/copy-time": "workspace:*",
    "@mpv-easy/hook": "workspace:*",
    "@mpv-easy/i18n": "workspace:*",
    "@mpv-easy/mock": "workspace:*",
    "@mpv-easy/plugin": "workspace:*",
    "@mpv-easy/polyfill": "workspace:*",
    "@mpv-easy/thumbfast": "workspace:*",
    "@mpv-easy/tool": "workspace:*",
    "@mpv-easy/ui": "workspace:*",
    "@polkadot/x-textencoder": "^12.6.2",
    "@react-spring/rafz": "^9.7.3",
    "@reduxjs/toolkit": "^2.1.0",
    "@rematch/core": "^2.2.0",
    "@rematch/select": "^3.1.2",
    "@types/react-dom": "^18.2.18",
    "chalk": "^5.3.0",
    "e-color": "^0.1.3",
    "firacode-icon": "^0.1.0",
    "lodash-es": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-reconciler": "^0.29.0",
    "react-redux": "^9.1.0",
    "react-use": "^17.5.0",
    "redux": "^5.0.1"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.12",
    "@types/react": "^18.2.55",
    "@types/react-reconciler": "^0.28.8",
    "@types/react-redux": "^7.1.33"
  },
  "browserslist": [
    "iOS >= 9",
    "Android >= 4.4",
    "last 2 versions",
    "> 0.2%",
    "not dead"
  ]
}
